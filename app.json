{
  "expo": {
    "name": "Shinobi",
    "slug": "Shinobi",
    "description": "Upload a video-game clip",
    "version": "1.2.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "assets/notification-icon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "cc.shinobi.android",
      "permissions": ["CAMERA"],
      "googleServicesFile": "./google-services.json",
      "versionCode": 35,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.shinobi.cc",
              "pathPrefix": "/profile"
            },
            {
              "scheme": "https",
              "host": "*.shinobi.cc",
              "pathPrefix": "/clip"
            },
            {
              "scheme": "https",
              "host": "*.shinobi.cc",
              "pathPrefix": "/notifications"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "ios": {
      "bundleIdentifier": "cc.shinobi.ios",
      "icon": "./assets/ios-icon.png",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.680842267587-peo8k5kmn7a1kub8mlgi74u1dm2supt8"
        }
      }
    },
    "scheme": "shn",
    "plugins": [
      "react-native-fast-image",
      "expo-image-picker",
      {
        "photosPermission": "The app accesses your photos to let you update your profile picture."
      }
    ],
    "currentFullName": "@rakshith.aloori/Shinobi",
    "originalFullName": "@rakshith.aloori/Shinobi"
  }
}
