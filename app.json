{
  "expo": {
    "name": "Shinobi",
    "slug": "Shinobi",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "assets/notification-icon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "cc.shinobi.android",
      "permissions": [],
      "googleServicesFile": "./google-services.json",
      "versionCode": 2
    },
    "scheme": "shn",
    "plugins": ["sentry-expo"],
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps"
        }
      ]
    }
  }
}
