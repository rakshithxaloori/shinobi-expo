{
  "expo": {
    "name": "Shinobi",
    "slug": "Shinobi",
    "description": "Upload a game clip",
    "version": "1.2.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "notification": {
      "icon": "assets/notification-icon.png"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "cc.shinobi.android",
      "permissions": ["CAMERA"],
      "googleServicesFile": "./google-services.json",
      "versionCode": 20,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.shinobi.cc",
              "pathPrefix": "/s"
            },
            {
              "scheme": "https",
              "host": "*.shinobi.cc",
              "pathPrefix": "/clip"
            },
            {
              "scheme": "https",
              "host": "*.shinobi.cc",
              "pathPrefix": "/match"
            },
            {
              "scheme": "https",
              "host": "oauth.shinobi.cc",
              "pathPrefix": "/instagram/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "scheme": "shn",
    "plugins": [
      "react-native-fast-image",
      "expo-image-picker",
      {
        "photosPermission": "The app accesses your photos to let you update your profile picture."
      }
    ],
    "currentFullName": "@rakshith.aloori/Shinobi",
    "originalFullName": "@rakshith.aloori/Shinobi"
  }
}
